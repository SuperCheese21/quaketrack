{
  "expo": {
    "name": "QuakeTrack",
    "slug": "quaketrack",
    "description": "Earthquake map and alerts app for Android and iOS built on React Native",
    "version": "0.6.0",
    "orientation": "portrait",
    "scheme": "quaketrack",
    "githubUrl": "https://github.com/SuperCheese21/quaketrack",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.stratospherelabs.quaketrack",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDjValuattV7Ox-ldmeg_eUaM9jNmJxnDY"
      }
    },
    "android": {
      "package": "com.stratospherelabs.quaketrack",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDjValuattV7Ox-ldmeg_eUaM9jNmJxnDY"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "icon": "./assets/ic_launcher-web.png",
    "notification": {
      "icon": "./assets/ic_notification.png"
    },
    "extra": {
      "eas": {
        "projectId": "72ef33c0-a9d4-11e8-8dfe-bbf74eaea179"
      }
    },
    "plugins": ["expo-font"]
  }
}
