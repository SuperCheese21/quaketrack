{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "prettier",
        "prettier/react",
        "plugin:you-dont-need-momentjs/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
        "fetch": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off"
    }
}
